Publisher: Pergamon-Elsevier Science Ltd
WOS:000780329000003},
	keywords = {Geophysics, Callisto, Deep interior, europa, Geodesy, Gravity and radio science, gravity-field, moment, satellites, shape, subsurface oceans, Tides, transponder},
	pages = {68--76},
}

