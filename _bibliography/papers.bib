	pages = {213--220},
	file = {Lognonné et al. - 2020 - Constraints on the shallow elastic and anelastic s.pdf:/Users/lunokhod/Zotero/storage/MRKM5LK9/Lognonné et al. - 2020 - Constraints on the shallow elastic and anelastic s.pdf:application/pdf;Lognonné et al. - 2020 - SI:/Users/lunokhod/Zotero/storage/LDC4RWNQ/Lognonné et al. - 2020 - SI.pdf:application/pdf},
