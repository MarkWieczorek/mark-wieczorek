	journal = {Reviews in Mineralogy and Geochemistry},
	author = {Wieczorek, Mark A and Weiss, Benjamin P and Breuer, Doris and Cébron, David and Fuller, Mike and Garrick-Bethell, Ian and Gattacceca, Jérôme and Halekas, Jasper S and Hemingway, Douglas J and Hood, Lon L and Laneuville, Matthieu and Nimmo, Francis and Oran, Rona and Purucker, Michael E and Rückriemen, Tina and Soderlund, Krista M and Tikoo, Sonia M},
