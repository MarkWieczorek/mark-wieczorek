@article{Hikida.Wieczorek2007,
	title = {Crustal thickness of the {Moon}: {New} constraints from gravity inversions using polyhedral shape models},
	volume = {192},
	copyright = {All rights reserved},
	issn = {00191035},
	shorttitle = {Crustal thickness of the {Moon}},
