	author = {Soderblom, Jason M. and Evans, Alexander J. and Johnson, Brandon C. and Melosh, H. Jay and Miljković, Katarina and Phillips, Roger J. and Andrews‐Hanna, Jeffrey C. and Bierson, Carver J. and Head, James W. and Milbury, Colleen and Neumann, Gregory A. and Nimmo, Francis and Smith, David E. and Solomon, Sean C. and Sori, Michael M. and Wieczorek, Mark A. and Zuber, Maria T.},
	month = sep,
	year = {2015},
	pages = {6939--6944},
	file = {Soderblom et al. - 2015 - The fractured Moon Production and saturation of p.pdf:/Users/lunokhod/Zotero/storage/3CLXI4X9/Soderblom et al. - 2015 - The fractured Moon Production and saturation of p.pdf:application/pdf},
