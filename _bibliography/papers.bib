	abstract = {A fundamental linear relationship exists between the eﬃciency of thermoremanent magnetization measured at room temperature and the magnitude of the ambient ﬁeld at the time of acquisition. The magnetic eﬃciency (the ratio of thermoremanent magnetization to saturation remanent magnetization) multiplied by the saturation magnetization is proportional to the magnetizing ﬁeld, where the proportionality constant is independent of magnetic mineralogy and domain state. The empirical constant for this equation was determined using existing experimental data of single domain through pseudosingle domain to multidomain states of iron, meteoritic iron, magnetite, maghemite, pyrrhotite, and hematite. We show that the acquired magnetic eﬃciency is closely related to two types of demagnetizing ﬁelds that act as barriers against domain wall pinning during magnetic acquisition. The ﬁrst relates to the saturation magnetization that is derived from the distribution of Bohr magnetons within the crystal lattice, and the second originates from grain shape. Theoretical considerations imply a factor of two diﬀerence in the magnetic eﬃciency acquired during laboratory and geologic timescales. This equation reveals that troilite may be a potentially important magnetic carrier for extraterrestrial magnetism. Using magnetic scanning techniques, our relationship allows for estimating the paleointensity from samples that contain more than one magnetic species.},
	language = {en},
	urldate = {2022-07-31},
	journal = {Physics of the Earth and Planetary Interiors},
	author = {Kletetschka, G. and Wieczorek, M.A.},
	month = nov,
	year = {2017},
	pages = {44--49},
	file = {Kletetschka and Wieczorek - 2017 - Fundamental relations of mineral specific magnetic.pdf:/Users/lunokhod/Zotero/storage/3AWNXGSN/Kletetschka and Wieczorek - 2017 - Fundamental relations of mineral specific magnetic.pdf:application/pdf},
}

@article{Broquet.etal2020,
	title = {Flexure of the {Lithosphere} {Beneath} the {North} {Polar} {Cap} of {Mars}: {Implications} for {Ice} {Composition} and {Heat} {Flow}},
	volume = {47},
	copyright = {All rights reserved},
	issn = {0094-8276, 1944-8007},
