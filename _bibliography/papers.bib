	file = {Broquet et al. - 2020 - Flexure of the Lithosphere Beneath the North Polar.pdf:/Users/lunokhod/Zotero/storage/JHMDTGDH/Broquet et al. - 2020 - Flexure of the Lithosphere Beneath the North Polar.pdf:application/pdf},
}

@article{Broquet.Wieczorek2019,
	title = {The {Gravitational} {Signature} of {Martian} {Volcanoes}},
	volume = {124},
