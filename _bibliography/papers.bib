	file = {Thorey et al. - 2015 - Gravitational signatures of lunar floor-fractured .pdf:/Users/lunokhod/Zotero/storage/BX68VWRM/Thorey et al. - 2015 - Gravitational signatures of lunar floor-fractured .pdf:application/pdf},
