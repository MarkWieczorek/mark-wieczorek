	pages = {7},
	file = {Jaumann et al. - 2022 - The Psyche Topography and Geomorphology Investigat.pdf:/Users/lunokhod/Zotero/storage/DU9EBYI3/Jaumann et al. - 2022 - The Psyche Topography and Geomorphology Investigat.pdf:application/pdf},
