	author = {Le Feuvre, M. and Wieczorek, M. A.},
	year = {2008},
	pages = {291--306},
	file = {Lefeuvre and Wieczorek - 2008 - Nonuniform cratering of the terrestrial planets.pdf:/Users/lunokhod/Zotero/storage/L5BCAKSK/Lefeuvre and Wieczorek - 2008 - Nonuniform cratering of the terrestrial planets.pdf:application/pdf},
