	file = {Elkins‐Tanton et al. - 2020 - Observations, Meteorites, and Models A Preflight .pdf:/Users/lunokhod/Zotero/storage/W692T22T/Elkins‐Tanton et al. - 2020 - Observations, Meteorites, and Models A Preflight .pdf:application/pdf},
