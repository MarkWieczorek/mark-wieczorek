	keywords = {dichotomy, Mars, marsquake, porosity, Rayleigh wave, surface wave},
	pages = {e2022GL101243},
	annote = {e2022GL101243 2022GL101243},
	file = {Full Text PDF:/Users/lunokhod/Zotero/storage/ESAUW8QW/Li et al. - 2023 - Different Martian Crustal Seismic Velocities Acros.pdf:application/pdf},
