	journal = {Geophysical Research Letters},
	author = {Tosi, N. and Čadek, O. and Běhounková, M. and Káňová, M. and Plesa, A.‐C. and Grott, M. and Breuer, D. and Padovan, S. and Wieczorek, M. A.},
	month = sep,
